# Konfigurasi Environment untuk Aplikasi Amprahan

# Frontend Configuration
REACT_APP_API_URL=http://localhost/amprahan/backend

# Database Configuration (untuk development)
DB_HOST=localhost
DB_NAME=amprahan
DB_USER=root
DB_PASS=

# JWT Secret (untuk production, ganti dengan secret yang kuat)
JWT_SECRET=your-secret-key-here

# Server Configuration
SERVER_PORT=3000
API_PORT=80

# Email Configuration (opsional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password 